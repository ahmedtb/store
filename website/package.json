{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "commit": "node -e \"const mssg = process.argv[1]; require('child_process').execSync('npm run prod && git add . && git commit -m \\\"' + mssg + '\\\" && git push', { stdio:[0, 1, 2] })\""
    },
